{
  "extends": "next/core-web-vitals",
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "@next/next/no-html-link-for-pages": "off"
  },
  "overrideConfig": {
    "linterOptions": {
      "reportUnusedDisableDirectives": "error"
    }
  }
} 